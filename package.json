  "tags": [
    "react",
    "typescript",
    "ui",
    "components",
    "library"
  ],
  "type": "module",
  "license": "MIT",
  "author": "Jonathan Cruz",
  "publishConfig": {
    "access": "public"
  },
  "files": [
